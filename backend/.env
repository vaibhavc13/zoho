PROJECT_NAME="Notion-Zoho-Cliq Integration"
API_V1_STR="/api/v1"

# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=notion_zoho_db
DATABASE_URL=postgresql://postgres:password@localhost/notion_zoho_db

# Security
